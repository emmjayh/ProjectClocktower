[flake8]
max-line-length = 160
extend-ignore = E203,W503,F401,F841,F541,C901
max-complexity = 25
exclude = 
    .git,
    __pycache__,
    *.pyc,
    .pytest_cache,
    venv,
    env
per-file-ignores =
    src/speech/*.py:F401,F841
    src/ai/*.py:F401,F841  
    *mock*.py:F401,F841
    tests/*.py:F401,F841